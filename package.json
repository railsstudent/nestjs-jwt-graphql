{
  "name": "nestjs-jwt-graphql",
  "version": "0.0.1",
  "description": "Create a nestjs GraphQL API that uses JWT authentication",
  "author": "Connie Leung",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "cspell": "cspell --no-must-find-files src/**/*.{ts,js}",
    "prepare": "husky install",
    "unimported": "unimported",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@nestjs/common": "9.0.9",
    "@nestjs/core": "9.0.9",
    "@nestjs/platform-express": "9.0.9",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.6"
  },
  "devDependencies": {
    "@commitlint/cli": "~17.0.2",
    "@commitlint/config-conventional": "~17.0.2",
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.1",
    "@nestjs/testing": "9.0.9",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.6",
    "@types/node": "16.0.0",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "cspell": "~6.1.1",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-rxjs": "~5.0.2",
    "eslint-plugin-sonarjs": "~0.13.0",
    "husky": "~8.0.1",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "nest-prettier": "0.0.4",
    "nestjs-lint-tools": "0.0.1",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "source-map-support": "0.5.21",
    "supertest": "6.2.4",
    "ts-jest": "28.0.7",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.7.4",
    "unimported": "1.21.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">= 16"
  }
}
